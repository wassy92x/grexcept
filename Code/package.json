    "browser",
    "anomaly",
    "try",
    "catch"
  ],
  "main": "dist/Index.js",
  "types": "dist/Index.d.ts",
  "author": "Fabian Wassenhoven <info@it-creators.de>",
  "license": "MIT",
  "engines": {
    "node": ">=6.4.0"
